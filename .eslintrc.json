{
    "plugins": ["@tanstack/query"],
    "extends": [
      "next/core-web-vitals",
      "plugin:@tanstack/eslint-plugin-query/recommended",
    ],
    "globals": {
      "React": "readonly"
    },
    "rules": {
      "no-unused-vars": [
        1,
        { "args": "after-used", "argsIgnorePattern": "^_" }
      ],
      "no-trailing-spaces": ["error", { "ignoreComments": true }]
    }
  }
  